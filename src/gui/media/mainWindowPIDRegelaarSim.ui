    border-radius: 10px;
    border: 2px solid #d1d5db;
    background-color: white;
}

/* Hover State */
QRadioButton::indicator:hover {
    border: 2px solid #3b82f6;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(255, 255, 255, 255),
