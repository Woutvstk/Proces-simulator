/* Radio Button Container */
QRadioButton {
    spacing: 8px;
    color: #1f2937;
    font-weight: 500;
}

/* Radio Button Indicator (Circle) */
QRadioButton::indicator {
    width: 20px;
    height: 20px;
    border-radius: 10px;
    border: 2px solid #d1d5db;
    background-color: white;
}

/* Hover State */
QRadioButton::indicator:hover {
    border: 2px solid #3b82f6;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(255, 255, 255, 255),
        stop:1 rgba(239, 246, 255, 255));
}

/* Checked State */
QRadioButton::indicator:checked {
    border: 2px solid #3b82f6;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(96, 165, 250, 255),
        stop:0.5 rgba(59, 130, 246, 255),
        stop:1 rgba(37, 99, 235, 255));
}

/* Checked Hover State */
QRadioButton::indicator:checked:hover {
    border: 2px solid #2563eb;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(147, 197, 253, 255),
        stop:0.5 rgba(96, 165, 250, 255),
        stop:1 rgba(59, 130, 246, 255));
}

/* Pressed State */
QRadioButton::indicator:pressed {
    border: 2px solid #1d4ed8;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(29, 78, 216, 255),
        stop:0.5 rgba(30, 64, 175, 255),
        stop:1 rgba(23, 37, 84, 255));
}

/* Disabled State */
QRadioButton::indicator:disabled {
    border: 2px solid #e5e7eb;
    background-color: #f3f4f6;
}

QRadioButton:disabled {
    color: #9ca3af;
}</string>
